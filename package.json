{
  "name": "didactic",
  "version": "0.1.0",
  "description": "Eval/optimization framework for LLM workflows",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "evaluation",
    "optimization",
    "prompt-engineering",
    "ai",
    "anthropic",
    "claude"
  ],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@supabase/supabase-js": "^2.88.0",
    "chrono-node": "^2.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "levenshtein": "^1.0.5",
    "munkres-js": "^1.2.2",
    "openai": "^6.15.0"
  },
  "devDependencies": {
    "@types/levenshtein": "^1.0.4",
    "@types/node": "^22.0.0",
    "tsdown": "^0.18.1",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
